pipeline {
    agent any
    stages { 
        stage('build') {
            steps {
                echo 'Hello World'
         stage('deploy') {
            steps { 
                echo 'welcome to my jenkins  world'
          stage('test') {
            steps { 
                echo 'believe ur believeness'
           stage('deliver') {
            steps { 
                echo 'everyday come with new point'
                    } 
                  }   
               }     
            }
        }
    }
}
 
